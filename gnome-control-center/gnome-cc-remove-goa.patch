-    }
-
-  if (self->accounts)
-    {
-      g_list_free_full (self->accounts, g_object_unref);
-      self->accounts = NULL;
-    }
-
-  G_OBJECT_CLASS (cc_background_grilo_miner_parent_class)->dispose (object);
-}
-
